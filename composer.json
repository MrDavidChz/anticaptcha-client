{
  "name": "gladyshev/anticaptcha-client",
  "description": "Full featured anti-captcha.com client library",
  "keywords": ["captcha", "anticaptcha", "anti-captcha"],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Dmitry Gladyshev",
      "email": "deel@email.ru"
    }
  ],
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "~6.3",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Anticaptcha\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Anticaptcha\\Test\\": "test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": "phpunit"
  }
}
